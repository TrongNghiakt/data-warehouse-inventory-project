                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    d.ProductID                            AS ProductAlternateKey,
    CONVERT(INT,FORMAT(h.OrderDate,'yyyyMMdd')) AS OrderDateKey,
    CONVERT(INT,FORMAT(h.DueDate,'yyyyMMdd'))   AS DueDateKey,
    CONVERT(INT,FORMAT(h.ShipDate,'yyyyMMdd'))  AS ShipDateKey,
    c.StoreID                               AS ResellerAlternateKey,
    h.SalesPersonID                         AS EmployeeAlternateKey,
    edh_current.DepartmentID                AS OrganizationAlternateKey,
    d.SpecialOfferID                        AS PromotionAlternateKey,
    cr.ToCurrencyCode                       AS CurrencyCode,
    h.TerritoryID                           AS SalesTerritoryAlternateKey,
    h.SalesOrderNumber,
    d.SalesOrderDetailID                    AS SalesOrderLineNumber,
    h.RevisionNumber,
    d.OrderQty                              AS OrderQuantity,
    d.UnitPrice,
    d.LineTotal                             AS ExtendedAmount,
    d.UnitPriceDiscount                     AS UnitPriceDiscountPct,
    d.LineTotal * d.UnitPriceDiscount       AS DiscountAmount,
    p.StandardCost                          AS ProductStandardCost,
    p.StandardCost * d.OrderQty             AS TotalProductCost,
    (d.LineTotal - d.LineTotal * d.UnitPriceDiscount) AS SalesAmount,
    h.TaxAmt                                AS TaxAmount,
    h.Freight,
    d.CarrierTrackingNumber,
    h.PurchaseOrderNumber                   AS CustomerPONumber
FROM Sales.SalesOrderHeader AS h
INNER JOIN Sales.SalesOrderDetail   AS d
    ON h.SalesOrderID = d.SalesOrderID
LEFT JOIN Sales.Customer             AS c
    ON h.CustomerID = c.CustomerID
LEFT JOIN Production.Product         AS p
    ON d.ProductID = p.ProductID
LEFT JOIN Sales.CurrencyRate         AS cr
    ON h.CurrencyRateID = cr.CurrencyRateID

-- chỉ lấy department hiện tại
LEFT JOIN (
    SELECT BusinessEntityID, DepartmentID
    FROM HumanResources.EmployeeDepartmentHistory
    WHERE EndDate IS NULL
) AS edh_current
    ON h.SalesPersonID = edh_current.BusinessEntityID

WHERE c.StoreID IS NOT NULL;</property>
