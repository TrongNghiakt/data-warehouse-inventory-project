                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    th.ProductID AS ProductKey,
    CONVERT(INT, CONVERT(CHAR(8), th.TransactionDate, 112)) AS DateKey,
    SUM(CASE 
            WHEN th.TransactionType IN ('P', 'R', 'W', 'I') -- Nhập kho
            THEN th.Quantity ELSE 0 
        END) AS UnitsIn,
    SUM(CASE 
            WHEN th.TransactionType IN ('S', 'M') -- Xuất kho
            THEN th.Quantity ELSE 0 
        END) AS UnitsOut,
    ISNULL(pi.Quantity, 0) AS QuantityOnHand,
    ISNULL(p.StandardCost, 0) AS UnitCost,
    ISNULL(pi.Quantity, 0) * ISNULL(p.StandardCost, 0) AS InventoryValue
FROM
(
    SELECT ProductID, TransactionDate, TransactionType, Quantity
    FROM Production.TransactionHistory

    UNION ALL

    SELECT ProductID, TransactionDate, TransactionType, Quantity
    FROM Production.TransactionHistoryArchive
) AS th
LEFT JOIN Production.ProductInventory pi
    ON th.ProductID = pi.ProductID
LEFT JOIN Production.Product p
    ON th.ProductID = p.ProductID
GROUP BY
    th.ProductID,
    CONVERT(INT, CONVERT(CHAR(8), th.TransactionDate, 112)),
    pi.Quantity,
    p.StandardCost
ORDER BY
    ProductKey,
    DateKey;</property>
