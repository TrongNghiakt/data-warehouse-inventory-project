                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  d.ProductID                      AS ProductKey,
  CONVERT(INT,CONVERT(CHAR(8),h.OrderDate,112))  AS OrderDateKey,
  CONVERT(INT,CONVERT(CHAR(8),h.DueDate,112))    AS DueDateKey,
  CONVERT(INT,CONVERT(CHAR(8),h.ShipDate,112))   AS ShipDateKey,
  h.CustomerID                    AS CustomerKey,
  d.SpecialOfferID                AS PromotionKey,
  cr.CurrencyRateID                AS CurrencyKey,
  cr.ToCurrencyCode          AS CurrencyCode,
  h.TerritoryID                   AS SalesTerritoryKey,
  h.SalesOrderNumber              AS SalesOrderNumber,
  d.SalesOrderDetailID            AS SalesOrderLineNumber,
  h.RevisionNumber                AS RevisionNumber,
  d.OrderQty                      AS OrderQuantity,
  d.UnitPrice                     AS UnitPrice,
  d.LineTotal                     AS ExtendedAmount,
  d.UnitPriceDiscount          AS UnitPriceDiscountPct,
  d.LineTotal * d.UnitPriceDiscount AS DiscountAmount,
  p.StandardCost                  AS ProductStandardCost,
  p.StandardCost * d.OrderQty     AS TotalProductCost,
  (d.LineTotal - d.LineTotal * d.UnitPriceDiscount) AS SalesAmount,
  h.TaxAmt                        AS TaxAmount,
  h.Freight                       AS Freight,
  d.CarrierTrackingNumber         AS CarrierTrackingNumber,
  h.PurchaseOrderNumber           AS CustomerPONumber
FROM Sales.SalesOrderHeader AS h
  INNER JOIN Sales.SalesOrderDetail AS d
    ON h.SalesOrderID = d.SalesOrderID
  INNER JOIN Production.Product AS p
    ON d.ProductID = p.ProductID 
  LEFT JOIN Sales.CurrencyRate cr on h.CurrencyRateID = cr.CurrencyRateID;</property>
